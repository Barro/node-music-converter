html.no-js(lang='en')
  //<![endif]
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    title
    meta(name='description', content='')
    meta(name='viewport', content='width=device-width')
    link(rel='stylesheet', href='frontend/style.css')
    //-script(src='javascripts/vendor/modernizr-2.5.3.min.js')
  body
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js')
    script
      window.jQuery || document.write('<script src="javascripts/vendor/jquery-1.8.2.min.js"><\\/script>')
    div(id="player")
      div(id="player-controls")
        button(id="play-control") Play
        button(id="next") Next!
        button(id="toggle-queue") Queue
        span(id="queue-length") 0
      audio
      input(class="position-slider", type="range", min="0", max="100", step="0.1")
      span(class="current-position") 00:00
      /
      span(class="duration") 00:00
      input(class="volume-slider", type="range", min="0", max="100", step="0.1")
      span(class="volume-intensity")
      div(id="status-current")
      div(id="status-next", style="color: grey")

    table(id="playlist", class="display playlist")

    //-script(src='javascripts/plugins.js')
    script(src='external/js/jquery.dataTables.min.js')
    script(src='external/js/Scroller.min.js')
    script(src='external/js/underscore.min.js')
    script(src='external/js/backbone.min.js')
    script(src='external/js/underscore.string.min.js')
    script(src='frontend/script.js')
